<%= form_tag sessions_path do %>
<div class="row">
  <div class="col-xs-2">
  <p><%=text_field_tag :email, nil, placeholder: "Email", class: "form-control" %></p>
  </div>
</div>
<div class="row">
  <div class="col-xs-2">
  <p><%=password_field_tag :password, nil, placeholder: "Password", class: "form-control" %></p>
  </div>
</div>
<div class="row">
  <div class="col-xs-2">
  <p><%= button_tag(type: "submit", class: "btn btn-default") do %>
    Login
<% end %></p>
  </div>
</div>
<%end%>