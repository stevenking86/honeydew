<%=form_for [@list, @task] do |f| %>
<div class="row">
  <div class="col-xs-2">
<p><%=f.text_field :description, placeholder: "Description", class: "form-control" %></p>
  </div>
</div>
<div class="row">
  <div class="col-xs-2">
<p><%= f.date_field :due_date, class: "form-control" %></p>
  </div>
</div>
<div class="row">
  <div class="col-xs-2">
<p>Assigned to: <%= select_tag :doers, options_for_select(@possible_doers.map {|doer| doer.name}), class: "form-control" %></p>
  </div>
</div>
<%= button_tag(type: "submit", class: "btn btn-default") do %>
    Add Task to List
<% end %>
<%end%>